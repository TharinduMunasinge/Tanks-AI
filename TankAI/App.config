<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>

  <appSettings>
    <add key="GuiIP" value="127.0.0.1" />
    <!--IP of the server-->
   
    <add key="GuiPort" value="20000" />
  <!--Port that the GUI listens to-->
    
    
    <add key="ServerIP" value="127.0.0.1" />
    <!--IP of the server-->
    <add key="ServerPort" value="6000" />
    <!--Port that the server listens to-->
    <add key="ClientPort" value="7000" />
    <!--Port that the client listens to-->
    <add key="MapSize" value="20" />
    <!--MapSize-->

  </appSettings>
  
</configuration>